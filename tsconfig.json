{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "strict": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "noImplicitOverride": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true,
    "outDir": "lib",
    "declaration": true
  },
  "include": [
    "src",
  ],
}
